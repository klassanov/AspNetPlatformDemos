@model IEnumerable<Demo3.Modules.Models.ModuleInfoModel>
<row>
    <div>
        <h1>Built-In Modules</h1>
    </div>
</row>
<row>
    <table class="table table-dark table-striped table-hover table-bordered">
        <tr>
            <th>#</th>
            <th>Module Name</th>
            <th>Module Type</th>
        </tr>
        @{
            int i = 0;
            foreach (var moduleInfo in Model)
            {
                <tr>
                    <td>@(++i)</td>
                    <td>@moduleInfo.Name</td>
                    <td>@moduleInfo.Type</td>
                </tr>
            }
        }
    </table>
</row>
